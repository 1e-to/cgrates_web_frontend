{{#bs-form model=model onSubmit=(action 'save') novalidate=true as |form|}}
  <div class='form-row'>
    {{form.element
      label='Tenant'
      property='tenant'
      class='col-md-6'
      data-test-tenant=true
    }}
    {{form.element
      label='ID'
      property='customId'
      class='col-md-6'
      data-test-custom-id=true
    }}
    {{form.element
      label='Filter IDs'
      property='filterIds'
      class='col-md-6'
      data-test-filter-ids=true
    }}
    {{form.element
      label='Filter field name'
      property='filterFieldName'
      class='col-md-6'
      data-test-filter-ield-name=true
    }}
    {{form.element
      label='Sorting'
      property='sorting'
      class='col-md-6'
      data-test-sorting=true
    }}
    {{form.element
      label='Activation interval'
      property='activationInterval'
      class='col-md-6'
      data-test-activation-nterval=true
    }}
    {{form.element
      label='Sorting params'
      property='sortingParams'
      class='col-md-6'
      data-test-sorting-params=true
    }}
    {{form.element
      label='Supplier ID'
      property='supplierId'
      class='col-md-6'
      data-test-supplier-id=true
    }}
    {{form.element
      label='Supplier Filter IDs'
      property='supplierFilterIds'
      class='col-md-6'
      data-test-supplier-filter-ids=true
    }}
    {{form.element
      label='upplier Account IDs'
      property='supplierAccountIds'
      class='col-md-6'
      data-test-supplier-account-ids=true
    }}
    {{form.element
      label='Supplier Rating plan IDs'
      property='supplierRatingplanIds'
      class='col-md-6'
      data-test-supplier-ratingplan-ids=true
    }}
    {{form.element
      label='Supplier Resource IDs'
      property='supplierResourceIds'
      class='col-md-6'
      data-test-supplier-resource-ids=true
    }}
    {{form.element
      label='Supplier Stat IDs'
      property='supplierStatIds'
      class='col-md-6'
      data-test-supplier-stat-ids=true
    }}
    {{form.element
      label='Supplier weight'
      property='supplierWeight'
      class='col-md-6'
      data-test-supplier-weight=true
    }}
    {{form.element
      label='Weight'
      property='weight'
      class='col-md-6'
      data-test-weight=true
    }}
  </div>
  {{bs-button defaultText='Save' type='primary' buttonType='submit' data-test-submit-button=true}}
{{/bs-form}}